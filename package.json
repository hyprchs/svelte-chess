{
	"name": "@jacksonthall22/svelte-chess",
	"description": "Fully playable chess component for Svelte. Powered by @jacksonthall22/chess.ts logic, Chessground chessboard and optionally Stockfish chess AI.",
	"keywords": [
		"chess",
		"svelte",
		"chessground",
		"chess.js",
		"chess.ts",
		"stockfish",
		"chess-engine",
		"chessboard",
		"lichess",
		"ui",
		"typescript"
	],
	"version": "0.1.0",
	"license": "GPL-3.0",
	"homepage": "https://github.com/gtim/svelte-chess#readme",
	"bugs": "https://github.com/gtim/svelte-chess/issues",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.15.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.14.0",
		"@sveltejs/package": "^2.3.7",
		"@testing-library/svelte": "^5.2.6",
		"@testing-library/user-event": "^14.5.2",
		"@typescript-eslint/eslint-plugin": "^8.18.1",
		"@typescript-eslint/parser": "^8.18.1",
		"@vitest/coverage-v8": "^2.1.8",
		"@vitest/web-worker": "^2.1.8",
		"eslint": "^9.17.0",
		"eslint-plugin-svelte": "^2.46.1",
		"jsdom": "^25.0.1",
		"publint": "^0.2.12",
		"svelte": "^5.15.0",
		"svelte-check": "^4.1.1",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^6.0.5",
		"vitest": "^2.1.8"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@jacksonthall22/chess.ts": "^0.1.12",
		"@jacksonthall22/svelte-chessground": "^0.1.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3"
	}
}
